{
    "name": "nullform/telegram-gateway",
    "description": "PHP client for Telegram Gateway API",
    "type": "library",
    "keywords": [
        "telegram gateway api",
        "telegram gateway",
        "api",
        "telegram",
        "verification"
    ],
    "homepage": "https://github.com/nullform/telegram-gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Novikov"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Nullform\\TelegramGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nullform\\TelegramGateway\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
